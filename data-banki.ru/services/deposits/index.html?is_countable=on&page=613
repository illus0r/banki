				<script>
					banki.utils.domReady.push(function() {
						require(['jquery'], function($) {
							var $button = $('.footer__copyright__button');
							var $close = $button.find('.footer__copyright__button__close');

							if (!Modernizr.svg) {
								$('.footer__logo').attr('src', $(this).data('png'));
							}

							$button.on('click', function() {
								$button.addClass('footer__copyright__button--opened');
							});

							$close.on('click', function(event) {
								event.stopPropagation();
								$button.removeClass('footer__copyright__button--opened');
							});
						});
					});
				</script>
