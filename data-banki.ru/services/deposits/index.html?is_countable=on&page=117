	<script>
		banki.utils.domReady.push(function() {
			require(['jquery', 'inViewport'], function($) {
				var $filter = $('#js-all-banks-filter-typeahead');

				$filter.inViewport(function() {
					require(requireModules['ui.typeahead'], function() {
						({
							state: {
								api_list_loaded: false
							},

							ui: {
								$filter: $('#js-all-banks-filter-typeahead')
							},

                            config: {
                                regionId: ''
                            },

							init: function () {
								var self = this;

								this.ui.$filter.bind('focus', function () {
									if (!self.state.api_list_loaded) {
										self.getAPI(self.startTypeahead);
									}
								});
							},

                            startTypeahead: function (data) {
                                this.ui.$filter.typeahead({
                                    name: 'banks',
                                    local: data,
                                    limit: 10
                                });

                                this.ui.$filter.bind('typeahead:selected typeahead:autocompleted', function (event, result, name) {
                                    document.location.href = '/banks/bank/' + result._code + '/';
                                });

                                // Dirty hack for lost focus :(
                                this.ui.$filter.trigger('focus');
                            },

                            getAPI: function (callback) {
                                var self = this, params = {};

                                if (self.config.regionId) {
                                    params.region_id = [self.config.regionId];
                                }

                                $.ajax({
                                    url: '/api/',
                                    type: 'POST',
                                    dataType: 'json',
                                    data: JSON.stringify({
                                        'jsonrpc': '2.0',
                                        'method': 'bankInfo/getBankList',
                                        'params': params,
                                        'id': 'test'
                                    }),
                                    success: function (response) {
                                        if (response && response.result && response.result.data) {
                                            self.state.api_list_loaded = true;
                                            var parsed = self.parseAPI(response.result.data);
                                            callback.call(self, parsed);
                                        }
                                    },
                                    error: function () {
                                        /* TODO: Cool Error Handler */
                                    }
                                });
                            },

                            parseAPI: function (data) {
                                var result = [];

                                // Reformat array (Typeahead.js format)
                                $.each(data, function (iter, item) {
                                    result.push({
										_code: item.code,
                                        value: item.bank_name.replace(/&[a-z]+;/g, '') // Kill HTML entities
                                    });
                                });

                                return result;
                            }
						}).init();
					});
				});
			});
		});
	</script>
</section>
	</section>

		

