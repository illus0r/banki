	<script>
		banki.utils.domReady.push(function() {
			require(['jquery', 'inViewport'], function($) {
				var $widget = $('#responses-cities-links');

				$widget.inViewport(function() {
					var $pager = $widget.find('[data-pager]'),
						$items = $widget.find('[data-items]').children();

					require(['ui.mini-pager'], function(pager) {
						pager($pager, {
							items: $items,
							pageItemsCount: 10
						});
					});
				});
			});
		});
	</script>
